    "@tanstack/react-router": "1.19.1",
    "axios": "1.7.5",
    "form-data": "4.0.0",
    "framer-motion": "10.16.16",
    "pdbe-molstar": "^3.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "7.49.3",
    "react-icons": "5.0.1",
    "react-query": "3.39.3",
    "swagger-ui-react": "^5.17.14",
    "zustand": "4.5.0"
